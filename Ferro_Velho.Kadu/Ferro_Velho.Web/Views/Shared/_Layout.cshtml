<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Junk Yard</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jquerymask")
    @Scripts.Render("~/bundles/jquerymaskmoney")
    @Scripts.Render("~/bundles/jquerymaskpeso")
    @Scripts.Render("~/bundles/jqueryprice")
    @Scripts.Render("~/bundles/util")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @*<ul class="nav navbar-nav navbar-left">
                        @Html.ActionLink("Pagina Inicial", "Index", "Home"@*, new { area = "" }, new { @class = "navbar-brand" })
                    </ul>*@
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav navbar-left">
                    @*<li>@Html.ActionLink("Pagina Inicial", "Index", "Home")</li>
                        <li>@Html.ActionLink("Caixa", "Index", "Caixa")</li>
                            <li>@Html.ActionLink("Cliente", "Index", "Cliente")</li>
                            <li>@Html.ActionLink("Material", "Index", "Material")</li>
                            <li>@Html.ActionLink("Telefones", "Index", "Telefone")</li>
                            <li>@Html.ActionLink("Contato", "Contact", "Home")</li>
                            <li>@Html.ActionLink("Sobre", "About", "Home")</li>*@
                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Kadu</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jquerymask")
    @Scripts.Render("~/bundles/jquerymaskmoney")
    @Scripts.Render("~/bundles/jquerymaskpeso")
    @Scripts.Render("~/bundles/jqueryprice")
    @Scripts.Render("~/bundles/util")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
